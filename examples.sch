(define faceplant
  `(letrec ((nth (lambda (n l) (if n (funcall nth (- n 1) (cdr l)) (car l)))))
     (cons 0
      (lambda (s w)
	(let ((wmap (car w))
	      (here (car (cdr (car (cdr w))))))
	  (let ((x (car here))
		(y (cdr here)))
	    (letrec ((try (lambda (s)
				(let ((dx (if (= s 1) 1 (if (= s 3) -1 0)))
				      (dy (if (= s 2) 1 (if (= s 0) -1 0))))
				  (if (funcall nth (+ x dx) (funcall nth (+ y dy) wmap))
				      (cons s s)
				      (funcall try (if (= s 3) 0 (+ s 1))))))))
	      (funcall try s))))))))

(define simplesearch
